{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --force",
        "buildCommand": "DISABLE_ESLINT_PLUGIN=true npm run build",
        "zeroConfig": true
      }
    }
  ],
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true
  },
  "installCommand": "npm install --force",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "DISABLE_ESLINT_PLUGIN": "true",
    "NEXT_DISABLE_ESLINT": "true"
  }
} 